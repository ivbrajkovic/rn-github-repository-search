{
  "expo": {
    "name": "rn-github-repository-search",
    "slug": "github-repository-search",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rngithubrepositorysearch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ivbrajkovic.githubrepositorysearch"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ivbrajkovic.githubrepositorysearch"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "66e6d3b9-5632-456c-949b-22f54c4fe977"
      }
    },
    "owner": "ivbrajkovic",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/66e6d3b9-5632-456c-949b-22f54c4fe977"
    }
  }
}
